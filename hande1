while wait() do
    pcall(function()
        local ss = workspace.CurrentCamera.Arms["Right Arm"]:FindFirstChild('Welded') or workspace.CurrentCamera.Arms["Right Arm"]:FindFirstChild('Right Arm')
        if ss then
            ss.C0 = CFrame.new(2.12969744, -1.32324028, -1.0158577, 0.98112762, 0.19261995, 0.0169171393, 0.0400832221, -0.117015719, -0.992320895, -0.189161211, 0.974271536, -0.122528195)
        end
    end)
end
